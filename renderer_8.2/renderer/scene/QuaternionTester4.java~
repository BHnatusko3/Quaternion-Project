package renderer.scene;
import java.util.Arrays;
public class QuaternionTester4
{

   public static void main(String[] args)
   {
      System.out.println("Z");
      for (int i = 0; i <= 360; i++)
      {
         Quaternion q = Quaternion.fromRotationMatrix2(Matrix.rotateZ(i));
       
         System.out.println(q + " | " + Arrays.toString(q.toEulerAnglesDegXYZ()));
         
      }
   }
}
